-- Criar a GUI para exibir os objetos do Workspace
local player = game.Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

local screenGui = Instance.new("ScreenGui")
screenGui.Name = "WorkspaceViewer"
screenGui.Parent = playerGui

local workspaceLabel = Instance.new("TextLabel")
workspaceLabel.Size = UDim2.new(0, 600, 0, 400)
workspaceLabel.Position = UDim2.new(0, 10, 0, 10)
workspaceLabel.BackgroundColor3 = Color3.new(0.2, 0.2, 0.2)
workspaceLabel.TextColor3 = Color3.new(1, 1, 1)
workspaceLabel.TextScaled = true
workspaceLabel.TextYAlignment = Enum.TextYAlignment.Top
workspaceLabel.TextXAlignment = Enum.TextXAlignment.Left
workspaceLabel.Parent = screenGui

-- Função para listar os objetos no Workspace
local function listarObjetosNoWorkspace(obj, nivel)
    nivel = nivel or 0
    local espaco = string.rep(" ", nivel * 2)
    local texto = espaco .. "- " .. obj.Name .. "\n"
    
    -- Se o objeto for um container (Model, Folder, etc.), listar seus filhos
    if #obj:GetChildren() > 0 then
        for _, filho in pairs(obj:GetChildren()) do
            texto = texto .. listarObjetosNoWorkspace(filho, nivel + 1)
        end
    end
    
    return texto
end

-- Atualizar a GUI com a lista de objetos do Workspace
workspaceLabel.Text = "Conteúdo do Workspace:\n" .. listarObjetosNoWorkspace(game.Workspace)
